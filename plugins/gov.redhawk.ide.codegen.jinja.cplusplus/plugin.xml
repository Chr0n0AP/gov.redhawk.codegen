<?eclipse version="3.4"?>
<plugin>
   <extension
         point="gov.redhawk.ide.codegen.codegens">
      <codegen
            autoGenerate="false"
            class="gov.redhawk.ide.codegen.jinja.cplusplus.CplusplusGenerator"
            compiler="/usr/bin/gcc"
            compilerVersion="4.4.7"
            humanLanguage="EN"
            id="gov.redhawk.ide.codegen.jinja.cplusplus.CplusplusGenerator"
            language="C++"
            languageVersion="0"
            name="C++ Code Generator"
            notDefaultable="false"
            priority="high">
         <description>
            Integrated &apos;redhawk-codegen&apos; C++ code generator for components and devices.
         </description>
         <operatingSystem
               default="true"
               os="Linux">
         </operatingSystem>
         <processor
               default="true"
               proc_type="x86">
         </processor>
         <processor
               default="true"
               proc_type="x86_64">
         </processor>
         <componenttype
               type="resource">
         </componenttype>
         <componenttype
               type="service">
         </componenttype>
         <componenttype
               type="device">
         </componenttype>
         <componenttype
               type="loadabledevice">
         </componenttype>
         <componenttype
               type="executabledevice">
         </componenttype>
      </codegen>
      <codegen
            autoGenerate="false"
            class="gov.redhawk.ide.codegen.jinja.cplusplus.CplusplusOctaveGenerator"
            compiler="/usr/bin/gcc"
            compilerVersion="4.4.7"
            hasSettings="true"
            humanLanguage="EN"
            id="gov.redhawk.ide.codegen.jinja.cplusplus.CplusplusOctaveGenerator"
            language="C++"
            languageVersion="0"
            name="C++ Octave Code Generator"
            notDefaultable="false"
            priority="normal">
         <description>
            Integrated&apos;redhawk-codegen&apos; Octave C++ code generator for components.
         </description>
         <operatingSystem
               default="true"
               os="Linux">
         </operatingSystem>
         <processor
               default="true"
               proc_type="x86">
         </processor>
         <processor
               default="true"
               proc_type="x86_64">
         </processor>
         <componenttype
               type="resource">
         </componenttype>
      </codegen>
      <codegen
            autoGenerate="false"
            class="gov.redhawk.ide.codegen.jinja.cplusplus.CplusplusSharedLibraryGenerator"
            compiler="/usr/bin/gcc"
            compilerVersion="4.4.7"
            humanLanguage="EN"
            id="gov.redhawk.ide.codegen.jinja.cplusplus.CplusplusSharedLibraryGenerator"
            language="C++"
            languageVersion="0"
            name="C++ Shared Library Code Generator"
            notDefaultable="false"
            priority="normal">
         <description>
            Integrated&apos;redhawk-codegen&apos; C++ code generator for soft package libraries.
         </description>
         <operatingSystem
               default="true"
               os="Linux">
         </operatingSystem>
         <processor
               default="true"
               proc_type="x86">
         </processor>
         <processor
               default="true"
               proc_type="x86_64">
         </processor>
         <componenttype
               type="resource">
         </componenttype>
      </codegen>
      <codegen
            autoGenerate="false"
            class="gov.redhawk.ide.codegen.jinja.cplusplus.OctaveSharedLibraryGenerator"
            compiler="/usr/bin/gcc"
            compilerVersion="4.4.7"
            humanLanguage="EN"
            id="gov.redhawk.ide.codegen.jinja.cplusplus.OctaveSharedLibraryGenerator"
            language="C++"
            languageVersion="0"
            name="Octave Shared Library Code Generator"
            notDefaultable="false"
            priority="normal">
         <description>
            Integrated'redhawk-codegen' Octave code generator for soft package libraries.
         </description>
         <operatingSystem
               default="true"
               os="Linux">
         </operatingSystem>
         <processor
               default="true"
               proc_type="x86">
         </processor>
         <processor
               default="true"
               proc_type="x86_64">
         </processor>
         <componenttype
               type="resource">
         </componenttype>
      </codegen>
   </extension>
   <extension
         point="gov.redhawk.ide.codegen.codegenTemplate">
      <template
            class="gov.redhawk.ide.codegen.jinja.cplusplus.template.DefaultCppTemplate"
            codegenId="gov.redhawk.ide.codegen.jinja.cplusplus.CplusplusGenerator"
            hasSettings="false"
            id="redhawk.codegen.jinja.cpp.component.pull"
            name="Pull Port Data"
            notDefaultable="false"
            selectable="true"
            usesPortTemplates="false">
         <description>
            This template will queue all data inside each input port. It is the developers responsibility to pull the data from the port's queue for processing.
         </description>
         <componenttype
               type="resource">
         </componenttype>
         <componenttype
               type="device">
         </componenttype>
         <componenttype
               type="loadabledevice">
         </componenttype>
         <componenttype
               type="executabledevice">
         </componenttype>
      </template>
      <template
            class="gov.redhawk.ide.codegen.jinja.cplusplus.template.DefaultCppTemplate"
            codegenId="gov.redhawk.ide.codegen.jinja.cplusplus.CplusplusGenerator"
            hasSettings="false"
            id="redhawk.codegen.jinja.cpp.service"
            name="Default Service"
            notDefaultable="false"
            selectable="true"
            usesPortTemplates="false">
         <componenttype
               type="service">
         </componenttype>
         <description>
            Service code generator
         </description>
      </template>
      <template
            class="gov.redhawk.ide.codegen.jinja.cplusplus.template.DefaultCppTemplate"
            codegenId="gov.redhawk.ide.codegen.jinja.cplusplus.CplusplusOctaveGenerator"
            hasSettings="false"
            id="redhawk.codegen.jinja.cpp.component.octave"
            name="Octave Pull Port Data"
            notDefaultable="false"
            selectable="true"
            usesPortTemplates="false">
         <description>
            This template will queue all data inside each input port. It is the developers responsibility to pull the data from the port's queue for processing.
         </description>
         <componenttype
               type="resource">
         </componenttype>
      </template>
      <template
            class="gov.redhawk.ide.codegen.jinja.cplusplus.template.DefaultCppTemplate"
            codegenId="gov.redhawk.ide.codegen.jinja.cplusplus.CplusplusGenerator"
            hasSettings="false"
            id="redhawk.codegen.jinja.cpp.component.frontend"
            name="Front End Device Template"
            notDefaultable="true"
            selectable="true"
            usesPortTemplates="false">
         <componenttype
               type="device">
         </componenttype>
         <description>
            A template to aid in development of a Front End Interfaces compliant device.
         </description>
      </template>
      <template
            class="gov.redhawk.ide.codegen.jinja.cplusplus.template.SharedLibCppTemplate"
            codegenId="gov.redhawk.ide.codegen.jinja.cplusplus.CplusplusSharedLibraryGenerator"
            hasSettings="false"
            id="redhawk.codegen.jinja.cpp.library"
            name="Shared Library CPP Template"
            notDefaultable="false"
            selectable="true"
            usesPortTemplates="false">
         <componenttype
               type="resource">
         </componenttype>
         <description>
            This template will queue all data inside each input port. It is the developers responsibility to pull the data from the port's queue for processing.
         </description>
      </template>
      <template
            class="gov.redhawk.ide.codegen.jinja.cplusplus.template.DefaultCppTemplate"
            codegenId="gov.redhawk.ide.codegen.jinja.cplusplus.OctaveSharedLibraryGenerator"
            hasSettings="false"
            id="redhawk.codegen.jinja.project.softPackageDependency.directory"
            name="Shared Library Octave Template"
            notDefaultable="false"
            selectable="true"
            usesPortTemplates="false">
         <componenttype
               type="resource">
         </componenttype>
         <description>
            This template will queue all data inside each input port. It is the developers responsibility to pull the data from the port's queue for processing.
         </description>
      </template>
   </extension>
   <extension
         point="gov.redhawk.ide.debug.launchConfigurationFactories">
      <launchConfigurationFactory
            codegenref="gov.redhawk.ide.codegen.jinja.cplusplus.CplusplusGenerator"
            factory="gov.redhawk.ide.codegen.jet.cplusplus.CppLaunchConfigurationFactory"
            id="gov.redhawk.ide.codegen.jinja.cplusplus.launchConfigurationFactory"
            launchConfigType="gov.redhawk.ide.codegen.jet.cplusplus.launchComponentApplication">
      </launchConfigurationFactory>
      <launchConfigurationFactory
            codegenref="gov.redhawk.ide.codegen.jinja.cplusplus.CplusplusOctaveGenerator"
            factory="gov.redhawk.ide.codegen.jinja.cplusplus.OctaveLaunchConfigurationFactory"
            id="gov.redhawk.ide.codegen.jinja.octave.launchConfigurationFactory"
            launchConfigType="gov.redhawk.ide.codegen.jet.cplusplus.launchComponentApplication">
      </launchConfigurationFactory>
   </extension>
   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            id="gov.redhawk.ide.codegen.jinja.octave.launchComponentApplication"
            name="Local Octave Component">
      </launchConfigurationType>
   </extension>
   <extension
         id="Legacy JET C++ Generator Extension Points"
         name="Legacy JET C++ Generator Extension Points"
         point="gov.redhawk.ide.codegen.codegens">
      <codegen
            associatedPerspectiveId="org.eclipse.cdt.ui.CPerspective"
            autoGenerate="false"
            class="gov.redhawk.ide.codegen.jet.cplusplus.GeneratorStub"
            compiler="/usr/bin/gcc"
            compilerVersion="4.1.2"
            deprecated="true"
            humanLanguage="EN"
            id="gov.redhawk.ide.codegen.jet.cplusplus.CplusplusGenerator"
            language="C++"
            languageVersion="0"
            name="Legacy JET C++ Code Generator"
            notDefaultable="false"
            priority="normal">
         <description>
            This is the Jet generator for C++ components and devices.
         </description>
         <operatingSystem
               default="true"
               os="Linux">
         </operatingSystem>
         <processor
               default="true"
               proc_type="x86">
         </processor>
         <processor
               default="true"
               proc_type="x86_64">
         </processor>
         <componenttype
               type="resource">
         </componenttype>
         <componenttype
               type="device">
         </componenttype>
      </codegen>
   </extension>
   <extension
         id="Legacy JET C++ Template Extension Points"
         name="Legacy JET C++ Template Extension Points"
         point="gov.redhawk.ide.codegen.codegenTemplate">
      <template
            class="gov.redhawk.ide.codegen.jet.cplusplus.pattern.TemplateStub"
            codegenId="gov.redhawk.ide.codegen.jet.cplusplus.CplusplusGenerator"
            hasSettings="true"
            id="gov.redhawk.ide.codegen.jet.cplusplus.ManualTemplate"
            name="Manual Generator"
            notDefaultable="true"
            selectable="true"
            usesPortTemplates="false">
         <description>
            By default this only prepares the project for future C++ development and generates a basic component outline.
         </description>
         <property
               default_value="TRUE"
               key="generate_skeleton"
               name="Generate Skeleton Code"
               required="true">
            <description>
               Selecting this option will generate basic skeleton code.
            </description>
         </property>
         <componenttype
               type="resource">
         </componenttype>
         <componenttype
               type="device">
         </componenttype>
         <componenttype
               type="service">
         </componenttype>
      </template>
      <template
            class="gov.redhawk.ide.codegen.jet.cplusplus.pattern.TemplateStub"
            codegenId="gov.redhawk.ide.codegen.jet.cplusplus.CplusplusGenerator"
            hasSettings="true"
            id="gov.redhawk.ide.codegen.jet.cplusplus.PullPortDataTemplate"
            name="Pull Port Data"
            notDefaultable="false"
            selectable="true"
            usesPortTemplates="true">
         <description>
            This template will queue all data inside each input port. It is the developers responsibility to pull the data from the port's queue for processing.
         </description>
         <property
               default_value="FALSE"
               deprecated="true"
               key="auto_start"
               name="Auto Start the Component"
               required="true">
            <description>
               When this property is set, the generated code's initialize function is tied to the start function, which in turn starts the processing thread. 
	When the property is not set, the start function needs to be explicitly called to start the processing thread.
            </description>
         </property>
         <componenttype
               type="resource">
         </componenttype>
         <componenttype
               type="device">
         </componenttype>
      </template>
   </extension>

</plugin>
